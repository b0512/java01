public class Main {
    // Definindo a classe "Pessoa"
class car {
    // Atributos da classe
    private String marca; // Atributo privado para encapsulamento
    private String modelo;
    private String cor;
    private int ano;   // Atributo privado para encapsulamento
    private float velocidade;
    
    // Método construtor da classe para inicializar os atributos
    car(String marca,String modelo, String cor, int ano, float velocidade) {
        this.marca = marca;
        this. modelo = modelo;
        this. cor = cor;
        this. ano = ano;
        this.velocidade = velocidade;
    }
    
    // Método getter para obter o nome da pessoa
    public String getMarca() {
        return marca;
    }
    
    // Método getter para obter a idade da pessoa
    public int getModelo() {
        return modelo;
    }

    public int getCor() {
        return cor;
    }
    public int getAno() {
        return ano;
    }
    public int getVelocidade() {
        return velocidade;
    }
}

// Classe principal que contém o método main
public class Main {
    public static void main(String[] args) {
        // Criando um objeto da classe Pessoa chamado "pessoa1"
        Car car1 = new car("Fiat","Uno", "Preto" ,2018 ,200);
        
        // Usando os métodos getters para obter e imprimir as informações da pessoa
        System.out.println("MARCA: " + car1.getMarca()); // Obtém e imprime o nome da pessoa
        System.out.println("Modelo: " + car1.getModelo()); // Obtém e imprime a idade da pessoa
        System.out.println("Cor: " + car1.getCor()); // Obtém e imprime a idade da pessoa
        System.out.println("Ano: " + car1.getAno()); // Obtém e imprime a idade da pessoa
        System.out.println("Velocidade: " + ca1.getVelocidade()); // Obtém e imprime a idade da pessoa
    }
}

